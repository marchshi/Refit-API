<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.smq.demo5.bean">
	
	<class name="AssetRecordBean" table="asset_record">
		<id name="assetId" column="asset_id">
			<generator class="native"></generator>
		</id>
		<property name="accountId" column="account_id"></property>
		<property name="status" column="status"></property>
		<property name="price" column="price"></property>
		<property name="assetDate" column="asset_date"></property>
	</class>
</hibernate-mapping>
