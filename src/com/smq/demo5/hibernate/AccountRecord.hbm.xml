<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.smq.demo5.bean">
	
	<class name="AccountRecordBean" table="account_record">
		<id name="recordId" column="record_id">
			<generator class="native"></generator>
		</id>
		<property name="accountId" column="account_id"></property>
		<property name="status" column="status"></property>
		<property name="orderId" column="order_id "></property>
		<property name="payAccountId" column="pay_account_id"></property>
		<property name="price" column="price"></property>
		<property name="payDate" column="pay_date"></property>
	</class>
</hibernate-mapping>
